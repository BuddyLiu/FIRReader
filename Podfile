    pod 'JSONModel',              '~>1.7.0'     # 模型
    pod 'SDWebImage',             '~>4.3.0'     # 加载图片
    pod 'NullSafe',               '~>1.2.3'     # 判空处理
    pod 'IQKeyboardManager',      '~>5.0.7'     # 键盘弹出遮挡处理库

end
